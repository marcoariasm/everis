{
  "version": 1,
  "projects": {
    "affiliates": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/affiliates",
      "sourceRoot": "apps/affiliates/src",
      "prefix": "everis-afp-prima",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/affiliates",
            "index": "apps/affiliates/src/index.html",
            "main": "apps/affiliates/src/main.ts",
            "polyfills": "apps/affiliates/src/polyfills.ts",
            "tsConfig": "apps/affiliates/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/affiliates/src/favicon.ico",
              "apps/affiliates/src/assets"
            ],
            "styles": [
              "apps/affiliates/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/affiliates/src/environments/environment.ts",
                  "with": "apps/affiliates/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "affiliates:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "affiliates:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "affiliates:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/affiliates/tsconfig.app.json",
              "apps/affiliates/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/affiliates/**/*"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/affiliates/jest.config.js",
            "tsConfig": "apps/affiliates/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "apps/affiliates/src/test-setup.ts"
          }
        }
      }
    },
    "affiliates-e2e": {
      "root": "apps/affiliates-e2e",
      "sourceRoot": "apps/affiliates-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/affiliates-e2e/cypress.json",
            "tsConfig": "apps/affiliates-e2e/tsconfig.e2e.json",
            "devServerTarget": "affiliates:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "affiliates:serve:production"
